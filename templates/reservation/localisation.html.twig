<div id="maCarte"></div>
</body>

<script src="https://unpkg.com/leaflet@1.7.1/dist/leaflet.js" integrity="sha512-XQoYMqMTK8LvdxXYG3nZ448hOEQiglfqkJs1NOQV44cWnUrBc8PkAOcXy20w0vlaXaVUearIOBhiXZ5V3ynxwA==" crossorigin=""></script>
<script>
    // Nous initialisons une liste de marqueurs
    var villes = {
        "Kelibia": { "lat": 36.8456561, "lon": 11.0935701 },
        "Nabeul": { "lat": 36.4512897, "lon": 10.7355915 },
        "Kerkouene": { "lat": 36.9562328, "lon": 11.0856801 },
        "AinDrahem": { "lat": 36.77364500, "lon": 8.68481470 },
        "Korbous": { "lat": 36.8166800, "lon": 10.5686500 },
        "Jendouba": { "lat": 36.507226, "lon": 8.775656 },
        "Haouaria": { "lat": 37.05002, "lon": 11.01442 },
        "DarAllouch": { "lat": 36.9672673, "lon": 11.0542052 },
        "Korba": { "lat": 36.5979471, "lon": 10.8463669 }
    };
    var macarte = L.map('maCarte').setView([36.8456561, 11.0935701],15);
    L.tileLayer('https://{s}.tile.openstreetmap.FR/osmfr/{z}/{x}/{y}.png', {
        // Il est toujours bien de laisser le lien vers la source des données
        attribution: 'données © OpenStreetMap/ODbL - rendu OSM France',
        minZoom: 1,
        maxZoom: 20
    }).addTo(macarte);

    //on personnalise le marqueur
    var icon = L.icon({
        iconUrl: "assets/icon.png",
        iconSize: [50,50],
        iconAnchor: [25,50]
    })


    // Nous parcourons la liste des villes
    for (ville in villes) {
        var marqueur = L.marker([villes[ville].lat, villes[ville].lon])
            .addTo(macarte);
        // Nous ajoutons un marqueur et on l'attribue une popup
        //var marqueur = L.marker([36.8456561, 11.0935701]).addTo(macarte);
        marqueur.bindPopup("<p>"+ville+"</p>");
    }

</script>
